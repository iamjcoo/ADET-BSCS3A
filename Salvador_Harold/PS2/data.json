{
    "htmlContent": "<!DOCTYPE html>\n<html lang='en'>\n<head>\n    <meta charset='UTF-8'>\n    <meta name='viewport' content='width=device-width, initial-scale=1.0'>\n    <title>Registration Form</title>\n    <link href='https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css' rel='stylesheet'>\n    <link rel='stylesheet' href='styles.css'>\n</head>\n<body>\n    <div class='container mt-5'>\n        <div class='row justify-content-center'>\n            <div class='col-md-8'>\n                <div class='card shadow-sm'>\n                    <img src='img/haikyuu.jpg'>\n                    <div class='card-body'>\n                        <form>\n                            <div class='form-row'>\n                                <div class='form-group col-md-4'>\n                                    <label for='firstName'>First Name</label>\n                                    <input type='text' class='form-control' id='firstName' placeholder='Juan'>\n                                </div>\n                                <div class='form-group col-md-4'>\n                                    <label for='middleName'>Middle Name</label>\n                                    <input type='text' class='form-control' id='middleName' placeholder='Smith'>\n                                </div>\n                                <div class='form-group col-md-4'>\n                                    <label for='lastName'>Surname</label>\n                                    <input type='text' class='form-control' id='lastName' placeholder='Dela Cruz'>\n                                </div>\n                            </div>\n                            <div class='form-group'>\n                                <label for='contactNumber'>Contact Number</label>\n                                <input type='tel' class='form-control' id='contactNumber' placeholder='09123456789'>\n                            </div>\n                            <div class='form-group'>\n                                <label for='email'>Email Address</label>\n                                <input type='email' class='form-control' id='email' placeholder='juandelacruz@gmail.com'>\n                            </div>\n                            <div class='form-group'>\n                                <label for='address'>Complete Address</label>\n                                <textarea class='form-control' id='address' rows='3' placeholder='Current Address'></textarea>\n                            </div>\n                            <button type='submit' class='btn btn-primary'>Register Now!</button>\n                        </form>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n    <script src='https://code.jquery.com/jquery-3.5.1.slim.min.js'></script>\n    <script src='https://cdn.jsdelivr.net/npm/@popperjs/core@2.5.4/dist/umd/popper.min.js'></script>\n    <script src='https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js'></script>\n</body>\n</html>",
    "cssContent": "body { background-color: rgb(153, 91, 37); background-repeat: no-repeat; background-size: auto; background-size: cover; margin: 7px; } h1 { background-color: auto; width: 300px; border: auto; padding: 48px; margin: 20px; margin: auto; position: relative; color: rgb(0, 0, 0); text-align: center; width: 60%; border: 3px; padding: 9px; position: absolute; top: 32%; left: 20%; } .card { border-radius: 10px; background-color: rgb(245, 245, 245); } .card-header { border-bottom: 1px solid #fdbaba; } .card-body { padding: 2rem; background-color: rgb(188, 156, 124); } .btn-primary { background-color: #944b03aa; border-color: #000000; } .btn-primary:hover { background-color: #d0d4d8; border-color: #dff091; }"
}
